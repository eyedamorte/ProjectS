{
  "name": "@lookingforaband/root",
  "version": "1.0.0",
  "description": "service for musician search",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "bootstrap": "yarn install",
    "// start scripts": "",
    "start:client": "turbo run start --filter=@lookingforaband/client",
    "start:server": "turbo run start --filter=@lookingforaband/server",
    "// build scripts": "",
    "build": "turbo run build",
    "build:client": "turbo run build --filter=@lookingforaband/client",
    "build:server": "turbo run build --filter=@lookingforaband/server",
    "// typecheck scripts": "",
    "typecheck": "turbo run typecheck",
    "typecheck:server": "turbo run typecheck --filter=@lookingforaband/server",
    "typecheck:client": "turbo run typecheck --filter=@lookingforaband/client",
    "// lint scripts": "",
    "lint": "turbo run lint",
    "lint:server": "turbo run lint --filter=@lookingforaband/server",
    "lint:client": "turbo run lint --filter=@lookingforaband/client"
  },
  "author": "eyedamorte",
  "license": "ISC",
  "dependencies": {
    "@eslint/js": "9.0.0",
    "@types/eslint": "8.56.9",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "ci-info": "4.0.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-turbo": "1.13.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-vitest": "0.5.3",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "turbo": "1.11.3",
    "typescript": "5.4.5"
  }
}
